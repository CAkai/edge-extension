{
  "manifest_version": 3,
  "default_locale": "zh_TW",
  "name": "__MSG_extensionName__",
  "version": "0.1.0",
  "description": "__MSG_extensionDescription__",
  "icons": {
    "16": "logo/16.png",
    "48": "logo/48.png",
    "128": "logo/128.png"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "storage",
    "sidePanel",
    "contextMenus"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "action": {
    "default_icon": "logo/48.png"
  },
  "options_ui": {
    "page": "src/pages/options/index.html"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content-script.ts-loader-BtKz00Ta.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "*.js",
        "*.css",
        "*.svg",
        "logo/128.png",
        "logo/48.png",
        "logo/16.png"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "<all_urls>",
        "http://*/*",
        "https://*/*"
      ],
      "resources": [
        "assets/content-script.ts-Ctig9oYa.js"
      ],
      "use_dynamic_url": false
    }
  ],
  "side_panel": {
    "default_path": "src/pages/side-panel/index.html"
  }
}
